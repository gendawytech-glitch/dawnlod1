RewriteEngine On

RewriteCond %{REQUEST_METHOD} OPTIONS
RewriteRule ^(.*)$ $1 [R=200,L]

<IfModule mod_php.c>
    php_value upload_max_filesize 128M
    php_value post_max_size 128M
    php_value max_execution_time 600
    php_value max_input_time 600
</IfModule>

<Files "*.php">
    Order Allow,Deny
    Allow from all
</Files>
